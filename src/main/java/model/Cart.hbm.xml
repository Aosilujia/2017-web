<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Cart" table="carts">
		<id name="id" type="int">
			<column name="User_id" />
			<generator class="assigned" />
		</id>
		<property name="cart" type="java.lang.String">
			<column name="Cart" />
		</property>
	</class>
</hibernate-mapping>